# baseURI: https://www.examplecompany.eu/data/trains
# imports: https://data-interop.era.europa.eu/era-vocabulary/ontology.ttl
# imports: https://ontology.tno.nl/logistics/federated/DigitalTwin
# imports: https://ontology.tno.nl/smart-rail.ttl

@prefix : <https://www.examplecompany.eu/data/trains#> .
@prefix DigitalTwin: <https://ontology.tno.nl/logistics/federated/DigitalTwin#> .
@prefix era: <http://data.europa.eu/949/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix smart-rail: <http://ontology.tno.nl/smart-rail#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://www.examplecompany.eu/data/trains>
  a owl:Ontology ;
  owl:imports <https://data-interop.era.europa.eu/era-vocabulary/ontology.ttl> ;
  owl:imports <https://ontology.tno.nl/logistics/federated/DigitalTwin> ;
  owl:imports <https://ontology.tno.nl/smart-rail.ttl> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
:Train_1
  a smart-rail:Train ;
  smart-rail:isDangerous false ;
  rdfs:label "Train 1" ;
.
:VehicleType_1
  a era:VehicleType ;
  era:hasParkingBrake true ;
  era:hasRegenerativeBrake false ;
  era:hasSandingPrevention false ;
  era:hasShuntingRestrictions false ;
  rdfs:label "Vehicle type 1" ;
.
:VehicleType_2
  a era:VehicleType ;
  era:energyMeterInstalled true ;
  era:hasCurrentLimitation false ;
  era:hasRegenerativeBrake true ;
  era:hasSandingPrevention false ;
  era:hasTrainIntegrityConfirmation true ;
  rdfs:label "Vehicle type 2" ;
.
:Vehicle_1
  a era:Vehicle ;
  a smart-rail:Train ;
  a DigitalTwin:Train ;
  era:vehicleType :VehicleType_1 ;
  rdfs:label "Vehicle 1" ;
.
:Vehicle_2
  a era:Vehicle ;
  a smart-rail:Train ;
  a DigitalTwin:Train ;
  era:vehicleType :VehicleType_2 ;
  rdfs:label "Vehicle 2" ;
.
