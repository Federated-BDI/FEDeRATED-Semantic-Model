@prefix : <http://tno.io/BusinessService#> .
@prefix Classifications: <http://tno.io/Classifications#> .
@prefix DigitalTwin: <http://tno.io/DigitalTwin#> .
@prefix Event: <http://tno.io/Event#> .
@prefix businessService: <http://tno.io/BusinessService#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://tno.io/BusinessService>
  rdf:type owl:Ontology ;
  owl:imports <http://tno.io/DigitalTwin> ;
  owl:imports <http://tno.io/Event> ;
  owl:imports <http://tno.io/PhysicalInfrastructure> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer" ;
  owl:versionInfo "TODO(1): Discuss Cargo" ;
  owl:versionInfo "TODO(1): Push to gitHub" ;
  owl:versionInfo "TODO(1): Sparql out the class hierarchy and proeprty domain and range in the Graph entirley" ;
  owl:versionInfo "TODO(1):Discuss DigitalTwin and its generic proeprties" ;
  owl:versionInfo "TODO(1):Populate the excel " ;
  owl:versionInfo "TODO(1):Write an Evaluation Plan" ;
.
businessService:ActorDigitalTwin
  rdf:type owl:ObjectProperty ;
  rdfs:domain businessService:logisticsActor ;
  rdfs:label "Actor DigitalTwin" ;
  rdfs:range DigitalTwin:DigitalTwin ;
  rdfs:subPropertyOf businessService:LogisticsActorProperties ;
.
businessService:ActorLocationRole
  rdfs:domain businessService:logisticsActor ;
  rdfs:range <http://tno.io/PhysicalInfrastructure#locationRole> ;
.
businessService:Booking
  rdf:type owl:Class ;
  rdfs:comment "Agreement with a limited validity period between two enterprises for one or more orders" ;
  rdfs:subClassOf businessService:BusinessTransaction ;
.
businessService:BusinessTransaction
  rdf:type owl:Class ;
  rdfs:comment "All data shared by interactions for executing a business service" ;
  rdfs:subClassOf owl:Thing ;
.
businessService:Consignee
  rdf:type owl:Class ;
  rdfs:comment "A consignee is the role of an enterprise where it is the operator and/or owner of a logistical function at a place with the role 'place of delivery'. A consignee can act as customer (e.g. Terms of Delivery is 'ex works')." ;
  rdfs:label "Consignee" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Consignor
  rdf:type owl:Class ;
  rdfs:comment "A consignor is the role of an enterprise where it is the operator and/or owner of a logistical function at a place with the role 'place of acceptance'. A consignor can act as customer." ;
  rdfs:label "Consignor" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Customer
  rdf:type owl:Class ;
  rdfs:comment "Logisitcs Customer" ;
  rdfs:label "Customer" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Driver
  rdf:type owl:Class ;
  rdfs:comment "The person responsible for operating a transport means, hired or employee of the operator of that transport means -- Can be Mater, Driver, Captain, Pilot" ;
  rdfs:label "Driver" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:FrameworkContract
  rdf:type owl:Class ;
  rdfs:comment "A contract with a long term validity period between any two enterprise with agreements of business service delivery" ;
  rdfs:label "Framework Contract" ;
  rdfs:subClassOf businessService:BusinessTransaction ;
.
businessService:InvolvedEvent
  rdf:type owl:ObjectProperty ;
  rdfs:domain businessService:BusinessTransaction ;
  rdfs:range Event:Event ;
.
businessService:LSPName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:logisticsServiceProvider ;
  rdfs:label "Logistic Service Provider Name" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf businessService:actorProperties ;
.
businessService:LSPWebSite
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Logistics Service Provider Website infomration (link)" ;
  rdfs:domain businessService:logisticsServiceProvider ;
  rdfs:label "Logistics Service Provider WebSite" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf businessService:actorProperties ;
.
businessService:Operator
  rdf:type owl:Class ;
  rdfs:comment "An enterprise exploiting a logistical function at a location" ;
  rdfs:label "Operator" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Order
  rdf:type owl:Class ;
  rdfs:comment "Agreement between two enterprise for actual execution of a business service according a booking or framework contract." ;
  rdfs:label "Order" ;
  rdfs:subClassOf businessService:BusinessTransaction ;
.
businessService:Organization
  rdf:type owl:Class ;
  rdfs:label "Organization" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Person
  rdf:type owl:Class ;
  rdfs:label "Person" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:TransportService
  rdf:type owl:Class ;
  rdfs:comment "A service provided by a logistics for transport of cargo" ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:TransshipmentService
  rdf:type owl:Class ;
  rdfs:comment "A service for transshipping cargo from one transport means to another" ;
  rdfs:label "Transshipment Service" ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:actorProperties
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "do not instantiate -- Only for grouping." ;
  rdfs:domain businessService:logisticsActor ;
.
businessService:bookingNumber
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:Booking ;
  rdfs:range xsd:string ;
.
businessService:customerName
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Logistics Service customer name" ;
  rdfs:domain businessService:customer ;
  rdfs:label "Logistics Service Provider WebSite" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf businessService:actorProperties ;
.
businessService:deviationDuration
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:TransportService ;
  rdfs:range xsd:decimal ;
.
businessService:location_detail
  rdf:type owl:ObjectProperty ;
  rdfs:domain businessService:logisticsService ;
  rdfs:range DigitalTwin:Location ;
.
businessService:logisticsActor
  rdf:type owl:Class ;
  rdfs:comment "The role of a Person is context dependant, and therefore should be described using the Actor class." ;
  rdfs:label "Logistics Actor" ;
.
businessService:logisticsService
  rdf:type owl:Class ;
  rdfs:comment "Any service that is provided to a customer by a logistics or transport service provider." ;
  rdfs:subClassOf owl:Thing ;
.
businessService:logisticsServiceGroupage
  rdf:type owl:Class ;
  rdfs:comment "A service for grouping of cargo into equipment" ;
  rdfs:label "Groupage Service" ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:logisticsServiceProvider
  rdf:type owl:Class ;
  rdfs:comment "The organization that provides the logistics service" ;
  rdfs:label "Logistics Service Provider - LSP" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:serviceName
  rdf:type owl:DatatypeProperty ;
.
businessService:service_detail
  rdf:type owl:ObjectProperty ;
  rdfs:domain businessService:BusinessTransaction ;
  rdfs:range businessService:logisticsService ;
.
businessService:storageServiceCargo
  rdf:type owl:Class ;
  rdfs:comment "A service for (temporary) storage of cargo. This may include special cargo storage such as cool storage, animal hotels, and dangerous goods storage." ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:storageServiceProduct
  rdf:type owl:Class ;
  rdfs:comment "A service for storage of products (warehousing)" ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:transportModality_detail
  rdf:type owl:ObjectProperty ;
  rdfs:domain businessService:TransportService ;
  rdfs:range Classifications:transportModality ;
.
businessService:transportMovement_detail
  rdf:type owl:ObjectProperty ;
  rdfs:domain businessService:TransportService ;
  rdfs:range Classifications:transportMovementType ;
.
businessService:transportServiceDurationAverage
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:TransportService ;
  rdfs:range xsd:decimal ;
.
