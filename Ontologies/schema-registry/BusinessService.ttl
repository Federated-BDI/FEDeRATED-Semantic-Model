# baseURI: http://tno.io/BusinessService
# imports: http://www.w3.org/2004/02/skos/core

@prefix : <http://tno.io/BusinessService#> .
@prefix businessService: <http://tno.io/BusinessService#> .
@prefix Classifications: <http://tno.io/Classifications#> .
@prefix DigitaTwin: <http://tno.io/DigitalTwin#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://tno.io/BusinessService>
  rdf:type owl:Ontology ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
businessService:BusinessTransaction
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
businessService:TransportService
  rdf:type owl:Class ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:logisticsService
  rdf:type owl:Class ;
  rdfs:subClassOf owl:Thing ;
.
businessService:logisticsActor
  rdf:type owl:Class ;
  rdfs:comment "The role of a Person is context dependant, and therefore should be described using the Actor class." ;
  rdfs:label "Logistics Actor" ;
.
businessService:logisticsServiceProvider
  rdf:type owl:Class ;
  rdfs:label "Logistics Service Provider - LSP" ;
  rdfs:comment "The organization that provides the logistics service" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Customer
  rdf:type owl:Class ;
  rdfs:label "Customer" ;
  rdfs:comment "Logisitcs Customer" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:piloting
  rdf:type businessService:logisticsService ;
.
businessService:storageServiceCargo
  rdf:type owl:Class ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:storageServiceProduct
  rdf:type owl:Class ;
  rdfs:subClassOf businessService:logisticsService ;
.
businessService:tarnsportServiceDuration
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:TransportService ;
  rdfs:range xsd:decimal ;
.
businessService:LSPName
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:logisticsServiceProvider ;
  rdfs:label "Logistic Service Provider Name" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf businessService:actorProperties ;
.
businessService:LSPWebSite
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Logistics Service Provider Website infomration (link)" ;
  rdfs:domain businessService:logisticsServiceProvider ;
  rdfs:label "Logistics Service Provider WebSite" ;
  rdfs:range xsd:string ;
  rdfs:subPropertyOf businessService:actorProperties ;
.
businessService:customerName
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "Logistics Service customer name" ;
  rdfs:domain businessService:customer ;
  rdfs:label "Logistics Service Provider WebSite" ;
  rdfs:range xsd:string ;
   rdfs:subPropertyOf businessService:actorProperties ;
  . 
  
 businessService:actorProperties
  rdf:type owl:DatatypeProperty ;
  rdfs:domain businessService:logisticsActor ;
  rdfs:comment  "do not instantiate -- Only for grouping." ;
.
businessService:Consignee
  rdf:type owl:Class ;
  rdfs:comment "the organization responsible for receiving the cargo" ;
  rdfs:label "Consignee" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
businessService:Consignor
  rdf:type owl:Class ;
  rdfs:comment "the organization responsible for sending the cargo" ;
  rdfs:label "Consignor" ;
  rdfs:subClassOf businessService:logisticsActor ;
.
