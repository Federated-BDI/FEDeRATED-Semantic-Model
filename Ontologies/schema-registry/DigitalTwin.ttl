:measure  a          owl:DatatypeProperty ;
        rdfs:domain  :generalCargo ;
        rdfs:range   xsd:integer .
    
    
:modality  a         owl:DatatypeProperty ;
        rdfs:domain  :Transportmeans ;
        rdfs:range   xsd:string .
  
  
:netLoad  a          owl:DatatypeProperty ;
